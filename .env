# ETL_IMAGE=da-assistant-etl:local
# MCP_IMAGE=da-assistant-osm-mcp:local
# DB_IMAGE=da-assistant-db:local

# Use DockerHub images
ASSISTANT_IMAGE=jeffgunderson/da-assistant:latest
ETL_IMAGE=jeffgunderson/da-assistant-etl:latest
MCP_IMAGE=jeffgunderson/da-assistant-osm-mcp:latest
DB_IMAGE=jeffgunderson/da-assistant-db:latest

# Host bind for published services (127.0.0.1 = host-only; change to 0.0.0.0 to allow external hosts)
ASSISTANT_BIND=127.0.0.1
ETL_BIND=127.0.0.1

# Host ports (change as needed)
ASSISTANT_HOST_PORT=8052
ETL_HOST_PORT=8888

# Container ports (usually unchanged)
ASSISTANT_CONTAINER_PORT=8052
ETL_CONTAINER_PORT=8888

# DB (internal-only)
DB_CONTAINER_PORT=5432
POSTGRES_USER=myuser
POSTGRES_PASSWORD=mypassword
POSTGRES_DB=mydb

# single DB URI used inside containers
DB_URI=postgresql://myuser:mypassword@da-assistant-db:5432/mydb

# MCP (internal-only)
MCP_CONTAINER_PORT=8080
MCP_URI=http://da-assistant-osm-mcp:8080/mcp

# Jupyter token (leave blank for no token in dev; don't use blank on public hosts)
JUPYTER_TOKEN=

# Streamlit should listen inside the container on 0.0.0.0
STREAMLIT_SERVER_ADDRESS=0.0.0.0
STREAMLIT_SERVER_PORT=8052